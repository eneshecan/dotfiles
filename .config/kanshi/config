profile single {
    output eDP-1 scale 2
}

profile dual {
    output eDP-1 scale 2 position 0,0
    output HDMI-A-1 position 1920,0
}

profile single_external {
    output eDP-1 disable
    output *
}

profile dual_external {
    output eDP-1 disable
    output "Dell Inc. DELL P2419H 8BD7QF3" position 0,0
    output * position 1920,0
}
